{
  "name": "unisoft-wysiwyg",
  "version": "0.0.1",
  "description": "WYSIWYG Editor based on DraftJS",
  "scripts": {
    "build": "tsc && node tools/build-css.js",
    "docs": "typedoc --out docs src",
    "lint": "eslint \"src/**/*\"",
    "format": "prettier --write src/**/*",
    "check-format": "prettier --check src/**/*"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UniSoftORG/wysiwyg.git"
  },
  "keywords": [
    "wysiwyg",
    "editor",
    "draftjs"
  ],
  "author": "Rade Ilijev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UniSoftORG/wysiwyg/issues"
  },
  "homepage": "https://github.com/UniSoftORG/wysiwyg#readme",
  "dependencies": {
    "draft-js": "^0.11.7"
  },
  "devDependencies": {
    "@types/draft-js": "^0.11.12",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "clean-css": "^5.3.2",
    "clean-css-cli": "^5.6.2",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.4.4",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  }
}
